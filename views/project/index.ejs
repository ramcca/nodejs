<!doctype html>
<html lang="en">
  <%- include('./../head'); %>
  <body>
    <div class="container">
      
        <%- include('./../menu') %>

        <div class="p-3 mb-2 bg-secondary text-white">project List.</div>
        <table class="table">
          <thead class="table-dark">
            <tr>
              <th scope="col">#</th>
              <th scope="col">Name</th>
              <th scope="col">Action</th>
            </tr>
          </thead>
          <tbody>
            <% for(i=0;i<projectdata.length;i++){ %>
              <tr>
                <th scope="col"><%=i+1 %></th>
                <th scope="col"><%=projectdata[i].name %></th>
                <th scope="col">Detail</th>
              </tr>
              <% } %>
           
          </tbody>
        </table>
    </div>
    <%- include('./../scripts') %>
  </body>
</html>